	string Title {
		get => isInitialized ? (string)DevExpress.Framework.DxCommandInterop.GetTitle(dxTarget).Result : title;
		set {
			if (isInitialized) {
				DevExpress.Framework.DxCommandInterop.SetTitle(dxTarget, value);
